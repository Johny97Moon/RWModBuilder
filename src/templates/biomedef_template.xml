<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <BiomeDef>
        <defName>{{def_name}}</defName>
        <label>{{label}}</label>
        <description>{{description}}</description>
        <workerClass>BiomeWorker</workerClass>
        <animalDensity>{{animal_density}}</animalDensity>
        <plantDensity>{{plant_density}}</plantDensity>
        <settlementSelectionWeight>{{settlement_selection_weight}}</settlementSelectionWeight>
        <movementDifficulty>{{movement_difficulty}}</movementDifficulty>
        <texture>{{texture_path}}</texture>
        <forageability>{{forageability}}</forageability>
        <foragedFood>{{foraged_food}}</foragedFood>
        <wildPlantRegrowDays>{{wild_plant_regrow_days}}</wildPlantRegrowDays>
        <soundsAmbient>
            <li>{{ambient_sound}}</li>
        </soundsAmbient>
        <diseaseMtbDays>{{disease_mtb_days}}</diseaseMtbDays>
        <diseases>
            <li>
                <diseaseInc>{{disease_inc}}</diseaseInc>
                <commonality>{{disease_commonality}}</commonality>
            </li>
        </diseases>
        <terrainsByFertility>
            <li>
                <terrain>{{terrain_def_name}}</terrain>
                <min>{{fertility_min}}</min>
                <max>{{fertility_max}}</max>
            </li>
        </terrainsByFertility>
        <terrainPatchMakers>
            <li>
                <perlinFrequency>{{perlin_frequency}}</perlinFrequency>
                <thresholds>
                    <li>
                        <terrain>{{patch_terrain}}</terrain>
                        <min>{{patch_min}}</min>
                        <max>{{patch_max}}</max>
                    </li>
                </thresholds>
            </li>
        </terrainPatchMakers>
        <baseWeatherCommonalities>
            <Clear>{{weather_clear}}</Clear>
            <Fog>{{weather_fog}}</Fog>
            <Rain>{{weather_rain}}</Rain>
            <DryThunderstorm>{{weather_dry_thunderstorm}}</DryThunderstorm>
            <RainyThunderstorm>{{weather_rainy_thunderstorm}}</RainyThunderstorm>
            <FoggyRain>{{weather_foggy_rain}}</FoggyRain>
            <SnowGentle>{{weather_snow_gentle}}</SnowGentle>
            <SnowHard>{{weather_snow_hard}}</SnowHard>
        </baseWeatherCommonalities>
        <wildAnimals>
            <{{animal_def_name}}>{{animal_commonality}}</{{animal_def_name}}>
        </wildAnimals>
        <wildPlants>
            <{{plant_def_name}}>{{plant_commonality}}</{{plant_def_name}}>
        </wildPlants>
    </BiomeDef>
</Defs>
